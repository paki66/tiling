import turtle
from math import *
t = turtle.Turtle()
def crtanje_6 ():
    t.right(60)
    for i in range(5):
       t.forward(10)
       t.left(60)
    t.forward(10)
    t.left(120)
    t.penup()
    t.forward(20)
    t.pendown()
    t.forward(10)
def novi_red(j):
    t.penup()
    t.setpos(-370, 310-j*sqrt(300))
    t.pendown()
t.penup()
t.setpos(-370, 310)
t.pendown()
for j in range (1, 35):    
    for i in range(25):
        print(crtanje_6())
    novi_red(j)


turtle.done()